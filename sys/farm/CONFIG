
LANG=C
export LANG

# helpers
minutes() { echo $(($1*60)); }
hours() { echo $(($1*60*60)); }
days() { echo $(($1*60*60*24)); }
hhmm() { echo $((`hours $1`+`minutes $2`)); }

# every 30 minutes
SLEEP=`hhmm 0 30`

PACKAGE=radare2

REMOTEDIR=""
[ -f ~/.r2farmrc ] && . ~/.r2farmrc

TARGETS="
	build
	bindist
	python
	python-dist
	mingw32
	android-arm
	android-x86
"

